algoritmo "Calcular  area de um terreno"

var

      largura, comprimento, area_terreno : real
inicio

      // Entrada  de dados
      leia (largura)
      leia (comprimento)
      
      
      // Processamento de dados
      area_terreno <- largura * comprimento
      
      // Saída de dados
      escreva(area_terreno)

fimalgoritmo